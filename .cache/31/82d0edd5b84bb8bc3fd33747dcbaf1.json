r\n  color: var(--text-highlight-color);\r\n  text-shadow: 0px 0px 3px var(--text-highlight-color);\r\n\r\n  overflow: hidden;\r\n  white-space: nowrap;\r\n\r\n  animation: animate 5s ease-in-out 2s forwards;\r\n}\r\n\r\n#root .title {\r\n  font-size: 60px;\r\n  font-weight: bold;\r\n  color: var(--main-text-color);\r\n  -webkit-text-stroke: 0.3px var(--main-text-color);\r\n\r\n  position: relative;\r\n  animation: glowing 8s infinite; /* Adjust the duration as needed */\r\n\r\n  margin-top: 3%;\r\n  z-index: 2;\r\n}\r\n\r\n#root h2 {\r\n  color: var(--main-text-color);\r\n  margin-top: 10px;\r\n  font-weight: bold;\r\n}\r\n/*--*/\r\n\r\n/*-------------------------APP-------------------------*/\r\n.App {\r\n  text-align: center;\r\n  font-weight: bold;\r\n\r\n  padding: 2em;\r\n  margin: 0;\r\n\r\n  width: var(--width-s);\r\n  height: 100%;\r\n\r\n  box-sizing: border-box;\r\n}\r\n/*-------------------------- WORD SQUARE ---------------------------*/\r\n.WordArea {\r\n  margin-top: 10%;\r\n  margin-bottom: 5%;\r\n  height: 15vw;\r\n  width: 90%;\r\n\r\n  /* background-color: black; */ /*for testing purposes*/\r\n  border-radius: 10%;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.WordHolder_outside {\r\n  border-radius: 10%;\r\n  border: 2px solid var(--main-text-color);\r\n  box-shadow: 0px 0px 3.5vh var(--main-text-color),\r\n    inset 0px 0px 4vh var(--main-text-color);\r\n  transition: box-shadow 1s ease;\r\n\r\n  height: 40%;\r\n  width: 80%;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.WordHolder_outside.shine {\r\n  box-shadow: 0px 0px 7.5vh var(--main-text-color),\r\n    inset 0px 0px 4vh var(--main-text-color);\r\n}\r\n\r\n.WordHolder_middle {\r\n  border-radius: 9.5%;\r\n  border: 2px solid var(--main-text-color);\r\n  box-shadow: 0px 0px 2vh var(--main-text-color), inset 0px 0px 3vh rgb(0, 0, 0);\r\n  background-color: var(--text-stroke-shadow);\r\n  height: 85%;\r\n  width: 98%;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.randomWord {\r\n  font-size: 8vh;\r\n  opacity: 0;\r\n  transition: opacity 0.5s ease;\r\n  text-shadow: 1px 1px 2px var(--main-text-color); /* Adjust the values as needed */\r\n}\r\n\r\n.randomWord.fade {\r\n  opacity: 1;\r\n}\r\n\r\n/*-------------------------GENERATE BUTTON-------------------------*/\r\n.ButtonHolder {\r\n  border-radius: 10px;\r\n\r\n  /* background-color: black; */\r\n\r\n  height: 6vh;\r\n  width: 50%;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.GenerateButton {\r\n  height: 70%;\r\n  width: 80%;\r\n\r\n  font-size: 1em;\r\n  font-weight: bold;\r\n  color: white;\r\n\r\n  border-radius: 2px;\r\n  background-color: #2f3030;\r\n  -webkit-text-stroke: 0.3px black;\r\n\r\n  box-shadow: 0 0 0 var(--main-text-color);\r\n  transition: box-shadow 0.1s ease; /* Add a transition for box-shadow */\r\n\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  z-index: 2;\r\n}\r\n\r\n.GenerateButton:hover {\r\n  box-shadow: 0 0 5px var(--main-text-color), 0 0 10px var(--main-text-color),\r\n    0 0 10px var(--main-text-color), 0 0 30px var(--main-text-color);\r\n  border: 1px solid var(--main-text-color);\r\n  color: var(--main-text-color);\r\n}\r\n"},"lineCount":211}},"error":null,"hash":"c815bba0a63ea58a82218d08a7e975ac","cacheData":{"env":{}}}